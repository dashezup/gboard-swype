<?xml version="1.0" encoding="utf-8"?>
<framework>
    <softkey_template content_description="$desc$" id="@id/softkey_template_symbol_bottom" layout="@layout/softkey_label_medium_bottom" multi_touch="false" popup_timing="@string/qwerty_popup_timing">
        <action data="$param_data$" intention="COMMIT" keycode="$param_keycode=SHORT_TEXT$" popup_label="$param_data$" type="PRESS" />
        <action data="$long_press_data$" intention="COMMIT" keycode="PLAIN_TEXT" popup_label="$long_press_data$" popup_layout="@attr/PopupBubbleRectangularLayout" type="LONG_PRESS" />
        <label location="@id/label_bottom" value="$param_data$" />
    </softkey_template>
    <softkey_template content_description="$desc$" id="@id/softkey_template_symbol_bottom_decode" layout="@layout/softkey_label_medium_bottom" multi_touch="false" popup_timing="@string/qwerty_popup_timing">
        <action data="$param_data$" intention="DECODE" keycode="$param_keycode=SHORT_TEXT$" popup_label="$param_data$" type="PRESS" />
        <action data="$long_press_data$" intention="DECODE" keycode="PLAIN_TEXT" popup_label="$long_press_data$" popup_layout="@attr/PopupBubbleRectangularLayout" type="LONG_PRESS" />
        <label location="@id/label_bottom" value="$param_data$" />
    </softkey_template>
</framework>