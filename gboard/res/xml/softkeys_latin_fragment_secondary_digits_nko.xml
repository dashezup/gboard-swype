            <softkey hint_label="߅" id="@id/softkey_fragment_superscript_digit_five" long_press_data="߅,5" />
            <softkey hint_label="߆" id="@id/softkey_fragment_superscript_digit_six" long_press_data="߆,6" />
            <softkey hint_label="߇" id="@id/softkey_fragment_superscript_digit_seven" long_press_data="߇,7" />
            <softkey hint_label="߈" id="@id/softkey_fragment_superscript_digit_eight" long_press_data="߈,8" />
            <softkey hint_label="߉" id="@id/softkey_fragment_superscript_digit_nine" long_press_data="߉,9" />
