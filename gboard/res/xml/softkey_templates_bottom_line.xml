        <softkey_template content_description="$desc$" id="@id/softkey_template_bottom_line_punctuation_bottom" layout="$param_layout$" multi_touch="false" popup_timing="@string/qwerty_popup_timing">
            <action data="$param_data$" intention="COMMIT" keycode="$param_keycode$" popup_label="$param_data$" type="PRESS" />
            <label location="@id/label_bottom" value="$param_data$" />
        </softkey_template>
        <softkey_template content_description="$desc$" id="@id/softkey_template_bottom_line_punctuation_bottom_popup_label_small" layout="$param_layout$" multi_touch="false" popup_timing="@string/qwerty_popup_timing">
            <action data="$param_data$" intention="COMMIT" keycode="$param_keycode$" popup_label="$param_data$" type="PRESS" />
            <action data="$long_press_data$" intention="COMMIT" keycode="PLAIN_TEXT" popup_label="$long_press_data$" popup_layout="@attr/PopupBubbleRectangularLabelSmallLayout" type="LONG_PRESS" />
            <label location="@id/label_bottom" value="$param_data$" />
        </softkey_template>
        <softkey_template content_description="$desc$" id="@id/softkey_template_bottom_line_punctuation" layout="$param_layout$" multi_touch="false" popup_timing="@string/qwerty_popup_timing">
            <action data="$param_data$" intention="COMMIT" keycode="$param_keycode$" popup_label="$param_data$" type="PRESS" />
            <action data="$long_press_data$" intention="COMMIT" keycode="$param_keycode$" popup_label="$long_press_data$" popup_layout="@attr/PopupBubbleRectangularLabelSmallLayout" type="LONG_PRESS" />
            <label location="@id/label" value="$param_data$" />
        </softkey_template>
    </softkeys>
</framework>