<?xml version="1.0" encoding="utf-8"?>
<framework>
    <include href="@xml/keymapping_fragment_punctuation_bottom_popup" />
    <key_mapping exclude_state="COMPOSING" state="SHOW_EMOJI_SWITCH_KEY">
        <mapping key_id="@id/softkey_switch_to_emoji" view_id="@id/key_pos_qwerty_globe" />
    </key_mapping>
    <key_mapping state="SHOW_EMOJI_SWITCH_KEY+COMPOSING">
        <mapping key_id="@id/softkey_ja_qwerty_question" view_id="@id/key_pos_qwerty_globe" />
    </key_mapping>
    <if japanese_use_tri_state_mode="true">
        <key_mapping>
            <mapping key_id="@id/softkey_ja_switch_to_tri_state_alphabet_qwerty" view_id="@id/key_pos_switch_to_symbol" />
        </key_mapping>
        <merge_key_mapping exclude_state="NO_SETTINGS_KEY">
            <merge key_id="@id/softkey_fragment_bottom_popup_settings" view_id="@id/key_pos_switch_to_symbol" />
        </merge_key_mapping>
        <merge_key_mapping state="SHOW_ONE_HANDED_MODE_SWITCH">
            <merge key_id="@id/softkey_fragment_popup_switch_to_left_handed_mode" view_id="@id/key_pos_switch_to_symbol" />
        </merge_key_mapping>
    </if>
    <else>
        <key_mapping>
            <mapping key_id="@id/softkey_switch_to_non_prime_keyboard" view_id="@id/key_pos_switch_to_symbol" />
        </key_mapping>
    </else>
    <key_mapping state="SHIFT">
        <mapping key_id="@id/softkey_latin_capital_letter_q" view_id="@id/key_pos_qwerty_q" />
        <mapping key_id="@id/softkey_latin_capital_letter_w" view_id="@id/key_pos_qwerty_w" />
