<?xml version="1.0" encoding="utf-8"?>
<framework>
    <include href="@xml/keymapping_fragment_secondary_symbols_arabic" />
    <if enable_secondary_symbols="true">
        <merge_key_mapping>
            <merge key_id="@id/softkey_fragment_superscript_tilde" view_id="@id/key_pos_3_0" />
            <merge key_id="@id/softkey_fragment_superscript_arabic_left_square_bracket" view_id="@id/key_pos_3_1" />
            <merge key_id="@id/softkey_fragment_superscript_arabic_right_square_bracket" view_id="@id/key_pos_3_2" />
            <merge key_id="@id/softkey_fragment_superscript_arabic_left_curly_bracket" view_id="@id/key_pos_3_3" />
            <merge key_id="@id/softkey_fragment_superscript_arabic_right_curly_bracket" view_id="@id/key_pos_3_4" />
            <merge key_id="@id/softkey_fragment_superscript_vertical_line" view_id="@id/key_pos_3_7" />
            <merge key_id="@id/softkey_fragment_superscript_circumflex_accent" view_id="@id/key_pos_3_8" />
            <if language="az-IR                   |doi-Arab                   |dv-MV                   |khw                   |ks-Arab                   |ps                   |sd-Arab                   |trw                   |pa-PK">
                <merge key_id="@id/softkey_fragment_superscript_less_than" view_id="@id/key_pos_3_5" />
                <merge key_id="@id/softkey_fragment_superscript_more_than" view_id="@id/key_pos_3_6" />
            </if>
            <else>
                <merge key_id="@id/softkey_fragment_superscript_arabic_left_pointing_double_angle_bracket" view_id="@id/key_pos_3_5" />
                <merge key_id="@id/softkey_fragment_superscript_arabic_right_pointing_double_angle_bracket" view_id="@id/key_pos_3_6" />
            </else>
        </merge_key_mapping>
    </if>
</framework>