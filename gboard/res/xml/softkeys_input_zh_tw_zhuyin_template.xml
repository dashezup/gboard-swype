<?xml version="1.0" encoding="utf-8"?>
<framework>
    <softkeys>
        <softkey_list splitter=" ">
            <softkey_template content_description="$desc$" id="@id/softkey_template_zhuyin_qwerty" layout="@layout/softkey_zhuyin_label_character_label_header" multi_touch="false" popup_timing="@string/qwerty_popup_timing">
                <action data="$press_data$" intention="DECODE" keycode="$keycode_normal=PLAIN_TEXT$" popup_label="$press_label$" type="PRESS" />
