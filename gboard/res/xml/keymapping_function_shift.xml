<?xml version="1.0" encoding="utf-8"?>
<framework>
    <key_mapping>
        <mapping key_id="@id/softkey_shift" view_id="@id/key_pos_shift" />
    </key_mapping>
    <key_mapping state="SHIFT">
        <mapping key_id="@id/softkey_shift_shifted" view_id="@id/key_pos_shift" />
    </key_mapping>
    <key_mapping state="SHIFT_LOCK">
        <mapping key_id="@id/softkey_shift_locked" view_id="@id/key_pos_shift" />
    </key_mapping>
    <key_mapping state="SHIFT_COMBO">
        <mapping key_id="@id/softkey_shift_shifted_combo" view_id="@id/key_pos_shift" />
    </key_mapping>
</framework>