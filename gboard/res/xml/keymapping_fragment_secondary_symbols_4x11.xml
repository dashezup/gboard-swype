        <merge_key_mapping>
            <merge key_id="@id/softkey_fragment_superscript_degree_sign" view_id="@id/key_pos_3_0" />
            <merge key_id="@id/softkey_fragment_superscript_left_square_bracket" view_id="@id/key_pos_3_1" />
            <merge key_id="@id/softkey_fragment_superscript_right_square_bracket" view_id="@id/key_pos_3_2" />
