            <softkey hint_label="\#" id="@id/softkey_digit_three" long_press_data="\#,³,¾,⅗,⅜" press_data="3" />
            <softkey hint_label="$" id="@id/softkey_digit_four" long_press_data="$,⁴,⅘" press_data="4" />
            <softkey hint_label="%" id="@id/softkey_digit_five" long_press_data="%,⁵,⅚,⅝" press_data="5" />
            <softkey hint_label="^" id="@id/softkey_digit_six" long_press_data="^,⁶" press_data="6" />
            <softkey hint_label="&amp;" id="@id/softkey_digit_seven" long_press_data="&amp;,⁷,⅞" press_data="7" />
            <softkey hint_label="*" id="@id/softkey_digit_eight" long_press_data="*,⁸" press_data="8" />
            <softkey hint_label="(" id="@id/softkey_digit_nine" long_press_data="(,⁹" press_data="9" />
            <softkey hint_label=")" id="@id/softkey_digit_zero" long_press_data="),⁰,ⁿ,∅" press_data="0" />
