        <softkey_list splitter=" " template_id="@id/softkey_template_label_header_simple">
            <softkey hint_label="&lt;" id="@id/softkey_lower_comma" long_press_data="&lt;" press_data="," />
        </softkey_list>
        <softkey_list splitter="," template_id="@id/softkey_template_fix_order_simple">
            <softkey hint_label="&quot;" id="@id/softkey_apostrophe" long_press_data="„,“,”,&quot;,‚,‘,’" press_data="'" />
        </softkey_list>
        <softkey id="@id/softkey_tab" layout="@layout/softkey_icon" multi_touch="false" popup_timing="@string/qwerty_popup_timing">
            <action data="\u0009" intention="COMMIT" keycode="TAB" type="PRESS" />
            <icon location="@id/icon" value="@attr/iconTabKey" />
        </softkey>
    </softkeys>
</framework>